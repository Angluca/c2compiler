module sys_time;

import c2 local;

type time_t c_long;
type suseconds_t c_long;

type Timeval struct {
    time_t tv_sec;
    suseconds_t tv_usec;
} @(cname="timeval")

type Timezone struct {
    c_int tz_minuteswest;
    c_int tz_dsttime;
} @(cname="timezone")

func c_int gettimeofday(Timeval* tv, Timezone* tz);

func c_int settimeofday(const Timeval* tv, const Timezone* tz);

