module test;

import stdio local;

fn void test_while(i32 loops) {
    i32 i = 0;
    while (i < loops) {
        i++;
        if (i == 3) continue;
        printf("loop %d\n", i);
        if (i == 5) break;
    }
}

fn void test_do(i32 loops) {
    i32 i = 0;
    do {
        if (i == 3) continue;
        printf("loop %d\n", i);
        if (i == 5) break;
        i++;
    } while (i < loops);
}

public fn i32 main(i32 argc, const char** argv) {
    test_while(argc);
    return 0;
}


